GET http://localhost:3000/api/pharmacies/49d396c1-36d1-470f-976c-357951adb344

# Update Pharmacy by Id
PUT http://localhost:3000/api/pharmacies/49d396c1-36d1-470f-976c-357951adb344 HTTP/1.1
content-type: application/json

{
  "pharmacy_name": "Walgreens",
  "city": "San Diego",
  "state": "California",
  "zip_code": 94003
}

# Delete Pharmacy by Id
DELETE http://localhost:3000/api/pharmacies/49d396c1-36d1-470f-976c-357951adb344 HTTP/1.1